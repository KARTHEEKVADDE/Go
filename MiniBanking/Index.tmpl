{{ define "Index" }}
  {{ template "Header" }}
    {{ template "Menu"  }}
    <h2> Registered Users</h2>
    <table border="1">
      <thead>
      <tr>
        <td>ID</td>
        <td>Name</td>        
        <td>Years Of Experience</td>
        <td>Email ID</td>
        <td>Project Name</td>
        <td>Designation</td>
        <td>Skillset</td>
        <td>Completed Trainings</td>
        <td>Project Aquired Skills</td>
        <td>Achievements</td>
        <td>Employee Status</td>
        <td>View</td>
        <td>Edit</td>
        <td>Delete</td>
      </tr>
       </thead>
       <tbody>
    {{ range . }}
      <tr>
        <td>{{ .Id }}</td>
        <td>{{ .Name }}</td>
        <td>{{ .Years Of Experience }}</td>
        <td>{{ .Email ID }}</td>
        <td>{{ .Project Name }}</td>
        <td>{{ .Designation }}</td>
        <td>{{ .Skillset }}</td>
        <td>{{ .Completed Trainings }}</td>
        <td>{{ .Project Aquired Skills }}</td>
        <td>{{ .Achievements }}</td>
        <td>{{ .Employee Status }}</td>

        <td><a href="/show?id={{ .Id }}">View</a></td>
        <td><a href="/edit?id={{ .Id }}">Edit</a></td>
        <td><a href="/delete?id={{ .Id }}">Delete</a><td>
      </tr>
    {{ end }}
       </tbody>
    </table>
  {{ template "Footer" }}
{{ end }}
